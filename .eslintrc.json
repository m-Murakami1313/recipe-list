{
  "extends": [
    "next/core-web-vitals",
    "plugin:import/recommended",
    "plugin:import/warnings",
    "prettier",
    "plugin:tailwindcss/recommended"
  ],
  "ignorePatterns": [
    "*.config.js"
  ],
  "rules": {
    "react-hooks/exhaustive-deps": "off",
    "import/order": [
      "error",
      {
        "alphabetize": {
          "order": "asc"
        }
      }
    ]
  }
}
